@model IEnumerable<NCSafety.Models.Equipment>

@{
    ViewBag.Title = "Index";
}
<br />
<h2>Equipment</h2>
<br />

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Lab.labBuilding)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eqName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Lab.labBuilding)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eqName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
